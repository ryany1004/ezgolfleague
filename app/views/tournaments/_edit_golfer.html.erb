  <div class="modal-content edit-golfer-modal">
    <div class="modal-body p-5">
      <div class="edit-golfer-modal__header">
        <div>
          <%= image_tag('1.png') %>
        </div>
        <div class="edit-golfer-modal__header__left pl-4">
          <h3>Craig Inesta</h3>
          <div class="dues-text">
            <div class="paid"></div><p>Tournament Dues Paid</p>
          </div>
          <%= link_to "Mark Dues Unpaid", "#remove", class: 'btn btn-outline-dark' %>
