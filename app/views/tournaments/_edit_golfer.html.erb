    <div class="modal-footer edit-golfer-modal__footer">
      <%= link_to "Remove Golfer", "#remove", class: 'btn btn-outline-dark' %>
      <%= link_to "Save", "#save", class: 'btn btn__ezgl-secondary' %>
    </div>
  </div>
</div>