      <div class="edit-golfer-modal__contests pt-3 pb-3">
        <div class="form-subheader">
          <h6>Contests</h6>
        </div>

        <div class="checkbox"><input type="checkbox">Net & Gross Skins<br></div>
        <div class="checkbox"><input type="checkbox">Net & Gross Skins<br></div>
        <div class="checkbox"><input type="checkbox">Net & Gross Skins<br></div>
        <div class="checkbox"><input type="checkbox">Net & Gross Skins<br></div>
