<div class="col-md-6">
  <div class="panel panel-default panel-success">
    <div class="panel-heading">
      <h3 class="panel-title"><%= name %> Tournament Teams <span class="badge"><%= teams.count %></span></h3>
    </div>
    <table class="table table-striped">
    	<thead>
    		<tr>
    			<th>Team Players</th>
          <th>Tee Time</th>
    		</tr>
    	</thead>
    	<tbody>
        <% teams.each do |team| %>
        <tr>
          <td><%= team_member_names(team[:name_data], false) %></td>
          <td><%= display_tee_time_or_position(team[:group]) unless team[:group].blank? %></td>
        </tr>
        <% end %>
    	</tbody>
    </table>
  </div>
</div>
