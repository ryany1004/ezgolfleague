<table class="table table-striped">
  <thead>
    <tr>
      <th>Rank</th>
      <th>Player</th>
      <th>Points</th>
      <th>Payouts</th>
    </tr>
  </thead>
  <tbody>
  <% rankings.each do |group| %>
    <tr>
      <td colspan="5"><strong><%= group.name %></strong></td>
    </tr>
    <% group.league_season_rankings.where("points > 0 OR payouts > 0").each do |ranking| %>
      <% cache [ranking] do %>
        <%= render partial: 'rankings', locals: {ranking: ranking} %>
      <% end %>
    <% end %>
  <% end %>
  </tbody>
</table>