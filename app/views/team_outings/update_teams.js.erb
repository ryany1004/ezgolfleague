selectedTeams = [];

<% @teams_signed_up.each do |team| %>
  <% @outing_index.each do |o| %>
    <% if o[:id] == team.id %>
    	specifyPlayersButton = "<%= escape_javascript link_to('Specify Players', league_tournament_day_team_path(@league, @tournament, @tournament_day, team), role: 'button', class: 'btn btn-info btn-xs') %>";
      removeButton = "<%= escape_javascript link_to('Remove', league_tournament_delete_day_teams_path(@league, @tournament, @tournament_day, matchup_id: @matchup.id, team_id: team.id), method: :delete, role: 'button', class: 'btn btn-danger btn-xs', data: { confirm: 'Are you sure you want to remove this registration? This will delete all scorecard information for this team for this tournament.' }) %>";
    <% end %>
  <% end %>

  selectedTeams.push({ name: "<%= team.name %>", id: "<%= team.to_param %>", specifyPlayersButton: specifyPlayersButton, removeButton: removeButton });
<% end %>

var elementsUpdated = removeSelectedTeamsFromDropdowns(selectedTeams);

placeButtonsForTeams(selectedTeams, elementsUpdated);